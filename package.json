{
  "name": "simplexify",
  "version": "0.1.0",
  "description": "A performance oriented tool for creating Delaunay triangulations and Voronoi diagrams",
  "keywords": [
    "voronoi",
    "delaunay"
  ],
  "main": "delaunay.js",
  "dependencies": {},
  "devDependencies": {
    "benchmark": "^1.0.0",
    "browserify": "^10.2.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.1.0",
    "gulp-changed": "^1.2.1",
    "gulp-connect": "^2.2.0",
    "mocha": "^2.2.5",
    "seedrandom": "^2.4.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "quotes": [
        2,
        "single"
      ],
      "space-infix-ops": [
        0
      ],
      "no-use-before-define": [
        2,
        "nofunc"
      ]
    }
  },
  "scripts": {
    "test": "gulp test",
    "benchmark": "gulp benchmark"
  },
  "author": "Dylan MacKenzie",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanmackenzie/simplexify"
  },
  "license": "MIT"
}
