{
  "name": "simplexify",
  "version": "1.0.0",
  "description": "A performance oriented tool for creating Delaunay triangulations and Voronoi diagrams",
  "main": "delaunay.js",
  "dependencies": {},
  "devDependencies": {
    "browserify": "^10.1.3",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.1.0",
    "gulp-connect": "^2.2.0",
    "mocha": "^2.2.4",
    "vinyl-source-stream": "^1.1.0"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "semi": [2, "never"],
      "quotes": [2, "single"],
      "space-infix-ops": [0],
      "no-use-before-define": [2, "nofunc"]
    }
  },
  "scripts": {
    "test": "gulp test",
    "benchmark": "gulp benchmark"
  },
  "author": "Dylan MacKenzie",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanmackenzie/simplexify"
  },
  "license": "MIT"
}
